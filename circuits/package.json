{
  "name": "@zk-agent/circuit",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "compile:issue_closed": "circom circuits/issue_closed.circom --r1cs --wasm --sym --c --output build -l node_modules",
    "compile": "mkdir -p out && npm run compile:issue_closed",
    "gen-input:issue": "npx tsx ../helper/src/generateInput.ts --email=./emls/issue_closed.eml --output=input",
    "gen-input": "mkdir -p input && npm run gen-input:issue",
    "clear": "rm -rf out input",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "chai": "^5.0.3",
    "circom_tester": "^0.0.20",
    "cross-env": "^7.0.3",
    "ffjavascript": "^0.2.63",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@zk-email/circuits": "^3.2.3",
    "@zk-email/helpers": "^3.1.3",
    "circomlib": "^2.0.5"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!chai/)",
      "node_modules/(?!(@zk-email)/)"
    ]
  }
}
